{
  "Logging": {
    "LogLevel": {
      "Default": "Trace",
      "Microsoft.AspNetCore": "Trace"
    }
  },
  "Cors": [
    {
      "Name": "localhost-origins",
      "Methods": [ "GET", "POST", "PUT", "DELETE", "OPTIONS" ],
      "Origins": [ "http://localhost", "http://localhost:3000", "http://localhost:5000" ],
      "Headers": [],
      "AllowAnyHeader": true,
      "AllowAnyMethod": false,
      "AllowAnyOrigin": false
    }
  ],
  "AllowedHosts": "*"
}
